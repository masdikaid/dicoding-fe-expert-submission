{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "comma-dangle": "off",
    "no-underscore-dangle": "off",
    "import/no-cycle": "off",
    "no-undef": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
